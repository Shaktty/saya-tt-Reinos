#ACTION {^%1 te causa una profunda {hemorragia|herid}a en {un|el|la|los|una} %2!}
{
    #va herida %2;
    #showme @pullRight{<faa>Herida: <afa>%2<109>}
};
#SUBS {^Vislumbras una oportunidad de ataque contra %1}
{
    @pullRight{<fca>Oportunidad! <faa>-> <adb>%1<109>}
};
#SUBST {^[%1 orbita a Eirea}
{
    @pullRight{<faa>---><afa>%1<109>}
};
#SUBSTIT {^[%1 sale de Eirea}
{
    @pullRight{<dba>%1<109><faa><--}
};
